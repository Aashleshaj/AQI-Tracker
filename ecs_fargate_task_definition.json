{
  "family": "aqi-tracker-task",
  "networkMode": "awsvpc",
  "executionRoleArn": "arn:aws:iam::082923708139:role/ecsTaskexecutionRoleAQI",
  "containerDefinitions": [
    {
      "name": "aqi-tracker",
      "image": "082923708139.dkr.ecr.ap-south-1.amazonaws.com/aqi-tracker:latest",
      "essential": true,
      "portMappings": [
        {
          "containerPort": 8501,
          "protocol": "tcp"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/aqi-tracker",
          "awslogs-region": "ap-south-1",
          "awslogs-stream-prefix": "ecs"
        }
      }
    }
  ],
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512"
}